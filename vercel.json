{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js"
      },
      {
        "src":"/comments",
        "dest": "/routes/commentRoutes.js"
      },
      {
        "src":"/notifications",
        "dest": "/routes/notificationRoutes.js"
      }
    ]
  }
  
  